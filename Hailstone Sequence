import math
x = int(input())

count = 0
if x > 0:
    while x != 1:
        print(x, end='\t')
        if x % 2 == 0:
            x = x // 2
            count += 1
            
        elif x % 2 != 0:
            x = x*3 + 1
            count += 1
        if count % 10 == 0:
            print()

print(1)
